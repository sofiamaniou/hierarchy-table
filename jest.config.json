{
    "clearMocks": true,
    "moduleDirectories": ["node_modules"],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "modulePaths": ["<rootDir>"],
    "setupFilesAfterEnv": ["./setupTests.ts"],
    "testEnvironment": "jsdom",
    "testRegex": "\\.spec\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": ["ts-jest", { "tsconfig": "./tsconfig.json" }],
      "^.+\\.jsx?$": "babel-jest"
    },
    "verbose": true
  }
  